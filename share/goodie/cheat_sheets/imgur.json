{
    "id": "imgur_cheat_sheet",
    "name": "imgur",
    "description": "Imgur shortcuts",
    "metadata": {
        "sourceName": "ImgurCheatSheet",
        "sourceUrl": "https://www.imgur.com"
    },
    "section_order": [
        "General",
        "Editing"
    ],
    "sections": {
        "General": [{
            "val": "Exit upload or stats menu",
            "key": "Esc"
        }, {
            "val": "Expand album",
            "key": "9"
        }, {
            "val": "Favorite",
            "key": "[Inser] or [0]"
        }, {
            "val": "Upvote",
            "key": "+"
        }, {
            "val": "Downvote",
            "key": "-"
        }, {
            "val": "Slideshow",
            "key": "[Ctrl] + [F9]"
        }, {
            "val": "Zoom",
            "key": "Enter"
        }, {
            "val": "Previous / Next image",
            "key": "Left / Right arrows"
        }],
        "Editing": [{
            "val": "Upload box",
            "key": "%"
        }, {
            "val": "join line below to the current one",
            "key": "J"
        }, {
            "val": "change (replace) entire line",
            "key": "cc"
        }, {
            "val": "change (replace) to the end of the word",
            "key": "cw"
        }, {
            "val": "change (replace) to the end of the line",
            "key": "c$"
        }, {
            "val": "delete character and substitute text",
            "key": "s"
        }, {
            "val": "delete line and substitute text (same as cc)",
            "key": "S"
        }, {
            "val": "transpose two letters (delete and paste)",
            "key": "xp"
        }, {
            "val": "undo",
            "key": "u"
        }, {
            "val": "redo",
            "key": "[Ctrl] + [r]"
        }, {
            "val": "repeat last command",
            "key": "."
        }]
    }
}

