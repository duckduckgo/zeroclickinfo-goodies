        "Etiquette": [
            {
                "key": "Hello",
                "val": "ਸਤ ਸ੍ਰੀ ਅਕਾਲ",
                "trn": "Sata srī akāl"
            },
            {
                "key": "Good Morning",
                "val": "ਸ਼ੁਭ ਸਵੇਰ",
                "trn": "Śubha savēra"
            },
            {
                "key" : "Good evening",
                "val" : "ਸਤ ਸ੍ਰੀ ਅਕਾਲ",
                "trn" : "Sata srī akāl"
             },
             {
                "key" : "Good night",
                "val" : "ਸ਼ੁਭ ਰਾਤ",
                "trn" : "Śubha rāat"
             },
             {
                "key" : "Sorry",
                "val" : "ਅਫਸੋਸ ਹੈ",
                "trn" : "Aphasōs hai"
             },
             {
                "key" : "Thank you",
                "val" : "ਤੁਹਾਡਾ ਧੰਨਵਾਦ",
                "trn" : "Tuhāḍā dhanavād"
             },
             {
                "key" : "Please",
                "val" : "ਕ੍ਰਿਪਾ",
                "trn" : "Kripā"
             },
             {
                "key" : "Welcome",
                "val" : "ਸੁਆਗਤ ਹੈ",
                "trn" : "Su'āgat hai"
             },
             {
                "key" : "How are you?",
                "val" : "ਤੁਸੀ ਕਿਵੇਂ ਹੋ?",
                "trn" : "Tusī kivēn hō?"
             },
             {
                "key" : "Good luck!",
                "val" : "ਖੁਸ਼ਕਿਸਮਤੀ!",
                "trn" : "Khuśakisamatī!"
             },
             {
                "key" : "Pleased to meet you",
                "val" : "ਤੁਹਾਨੂੰ ਮਿਲ ਕੇ ਖੁਸ਼ੀ ਹੋਈ",
                "trn" : "Tuhānū mil kē khuśī hō'ī"
             },
             {
                "key" : "Goodbye!",
                "val" : "ਅਲਵਿਦਾ!",
                "trn" : "Alavidā!"
             },
             {
                "key" : "Congratulations!",
                "val" : "ਵਧਾਈ !",
                "trn" : "Vadhā'ī!"
             }
        ],
        "Getting Help": [
             {
                "key" : "Call the police!",
                "val" : "ਪੁਲਸ ਨੂੰ ਬੁਲਾਓ!",
                "trn" : "Pulas nū bulā'ō!"
             },
             {
                "key" : "Can you speak more slowly, please?",
                "val" : "ਕਿਰਪਾ ਕਰਕੇ ਤੁਸੀ ਹੋਰ ਹੌਲੀ ਹੌਲੀ ਗੱਲ ਕਰ ਸਕਦੇ ਹੋ ?",
                "trn" : "Ki tusi kirpā karakē, hor holi holi gal kar sakde ho?"
             },
             {
                "key" : "Stop!",
                "val" : "ਰੂਕੋ!",
                "trn" : "Rūkō!"
             },
             {
                "key" : "Do you speak English?",
                "val" : "ਕੀ ਤੁਸੀ ਅੰਗ੍ਰੇਜ਼ੀ ਬੋਲਦੇ ਹੋ?",
                "trn" : "Kī tusī angrēzī bōladē hō?"
             },
             {
                "key" : "Excuse me!",
                "val" : "ਮੈਨੂੰ ਮਾਫ਼ ਕਰੋ!",
                "trn" : "Mainū māfa karō!"

             },
             {
                "key" : "I need a doctor!",
                "val" : "ਮੈਨੂੰ ਇੱਕ ਡਾਕਟਰ ਦੀ ਲੋੜ ਹੈ!",
                "trn" : "Mainū ika ḍoctor dī lōṛ hai!"
             },
             {
                "key" : "Call an ambulance!",
                "val" : "ਐੰਬੁਲੇਂਸ ਨੂੰ ਬੁਲਾਓ!",
                "trn" : "Ambulance nū bulā'ō!"
             },
             {
                "key" : "Help!",
                "val" : "ਮਦਦ ਕਰੋ!",
                "trn" : "Madada karō!"
             },
             {
                "key" : "Fire!",
                "val" : "ਅੱਗ!",
                "trn" : "Aga!"
             }
        ],
        "Basics" : [
             {
                "key" : "What's your name?",
                "val" : "ਤੁਹਾਡਾ ਨਾਮ ਕੀ ਹੈ?",
                "trn" : "Tuhāḍā nāam kī hai?"
             },
             {
                "key" : "My name is...",
                "val" : "ਮੇਰਾ ਨਾਮ ਹੈ...",
                "trn" : "Mērā nāam hai..."
             },
             {
                "key" : "How are you?",
                "val" : "ਤੁਸੀ ਕਿਵੇਂ ਹੋ?",
                "trn" : "Tusī kivēn hō?"
             },
             {
                "key" : "I'm good, thanks!",
                "val" : "ਮੈ ਚੰਗਾ ਹਾ, ਧੰਨਵਾਦ !",
                "trn" : "Mai changā hā, dhanavāda!"
             },
             {
                "key" : "How old are you?",
                "val" : "ਤੁਹਾਡੀ ਉਮਰ ਕੀ ਹੈ?",
                "trn" : "Tuhāḍī umara kī hai?"
             },
             {
                "key": "Hello",
