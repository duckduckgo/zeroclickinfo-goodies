{
    "id": "todoist_cheat_sheet",
    "name": "Todoist",
    "description": "Todoist task manager",
    "metadata": {
        "sourceName": "Todoist Help",
        "sourceUrl": "https://todoist.com/Help/KeyboardShortcuts"
    },
    "template_type": "keyboard",
    "section_order": [
        "Editing or adding items",
        "General shortcuts"
    ],
    "sections": {
        "Editing or adding items": [{
            "key":"Esc",
            "val":"Cancel current edit"
          }, {
            "key":"Enter",
            "val":"Save current item and append a new item below it"
          }, {
            "key":"[Ctrl] [Enter]",
            "val":"Save current item and append a new item above it"
          }, {
            "key":"[Shift] [Enter]",
            "val":"Save currently edited item and append a new item below it"
          }, {
            "key":"[Ctrl] [←]",
            "val":"Indent the item one time to the left"
          }, {
            "key":"[Ctrl] [→]",
            "val":"Indent the item one time to the right"
          }, {
            "key":"[Ctrl] [↑]",
            "val":"Save current item and edit item above it"
          }, {
            "key":"[Ctrl] [↓]",
            "val":"Save current item and edit item below it"
          }, {
            "key":"[Ctrl] [b]",
            "val":"Insert bold syntax"
          }, {
            "key":"[Ctrl] [i]",
            "val":"Insert italics syntax"
          }, {
            "key":"[Shift] [Click]",
            "val":"Complete and move to history a sub-task or a recurring task"
          }, {
            "key":"[Ctrl] [M]",
            "val":"Add a new quick comment"
          }],
        "General shortcuts": [{
            "key":"[Ctrl] [Click]",
            "val":"Select and update multiple tasks"
          }, {
            "key":"[Shift] [Click]",
            "val":"Select and update multiple tasks (alternative)"
          }, {
            "key":"[Click] on task",
            "val":"Edit the task"
          }, {
            "key":"a",
            "val":"Add a new item to the bottom"
          }, {
            "key":"[Shift] a",
            "val":"Add a new item to the top"
          }, {
            "key":"/",
            "val":"Move focus to query box"
          }, {
            "key":"s",
            "val":"Sort items by date"
          }, {
            "key":"p",
            "val":"Sort items by priority"
          }, {
            "key":"[1], [2], [3] or [4] while drag and dropping",
            "val":"Change a task's indent level"
          }]
    }
}
