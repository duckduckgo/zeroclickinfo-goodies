            {
                "key": "[Alt] [↑]",
                "val": "Move to next chat"
            },
            {
                "key": "[Alt] [↓]",
                "val": "Move to previous chat"
            },
            {
                "key": "[Ctrl] [f]",
                "val": "Search"
            },
            {
                "key": "[Ctrl] [q]",
                "val": "Quit Telegram"
            },
            {
                "key": "[ctrl] [l]",
                "val": "Lock Telegram (if local password is set)"
            },
            {
                "key": "[ctrl] [m]",
                "val": "Minimize"
            },
            {
                "key": "[ctrl] [w]",
                "val": "Minimize to system tray"
            },
