{
    "id": "w3m_cheat_sheet",
    "name": "w3m",
    "description": "Keyboard shortcuts for w3m,a text based Web browser",
    "metadata": {
        "sourceName": "SourceForge",
        "sourceUrl": "http://w3m.sourceforge.net/MANUAL#Key:orig"
    },
    "template_type": "keyboard",
    "section_order": [
        "Page/Cursor motion",
        "Hyperlink operation",
        "File/Stream operation",
        "Buffer operation",
        "Buffer selection mode",
        "Bookmark operation",
        "Search",
        "Mark operation",
        "Miscellany",
        "Line-edit mode"
    ],
    "sections": {
        "Page/Cursor motion": [{
            "val": "Forward page",
            "key": "[SPC], [C-v]"
        }, {
            "val": "Backward page",
            "key": "[b], [ESC] [v]"
        }, {
            "val": "Cursor right",
            "key": "[l], [C-f]"
        }, {
            "val": "Cursor left",
            "key": "[h], [C-b]"
        }, {
            "val": "Cursor down",
            "key": "[j], [C-n]"
        }, {
            "val": "Cursor up",
            "key": "[k], [C-p]"
        }, {
            "val": "Roll up one line",
            "key": "J"
        }, {
            "val": "Roll down one line",
            "key": "K"
        }, {
            "val": "Go to the beginning of line",
            "key": "[^], [C-a]"
        }, {
            "val": "Go to the end of line",
            "key": "[$], [C-e]"
        }, {
            "val": "Go to next word",
            "key": "w"
        }, {
            "val": "Go to previous word",
            "key": "W"
        }, {
            "val": "Shift screen right",
            "key": ">"
        }, {
            "val": "Shift screen left",
            "key": "<"
        }, {
            "val": "Shift screen one column right",
            "key": "."
        }, {
            "val": "Shift screen one column left",
            "key": ","
        }, {
            "val": "Go to the first line",
            "key": "[g], [M-<]"
        }, {
            "val": "Go to the last line",
            "key": "[G], [M->]"
        }, {
            "val": "Go to specified line",
            "key": "[ESC] [g]"
        }, {
            "val": "Move to the center line",
            "key": "Z"
        }, {
            "val": "Move to the center column",
            "key": "z"
        }, {
            "val": "Move to next hyperlink",
            "key": "TAB"
        }, {
            "val": "Move to previous hyperlink",
            "key": "[C-u], [ESC] [TAB]"
        }, {
            "val": "Move to the first hyperlink",
            "key": "\\["
        }, {
            "val": "Move to the last hyperlink",
            "key": "\\]"
        }],
        "Hyperlink operation": [{
           "val": "Follow hyperlink",
            "key": "RET"
        }, {
            "val": "Save link to file",
            "key": "[a], [ESC] [RET]"
        }, {
            "val": "Peek link URL",
            "key": "u"
        }, {
            "val": "Peek image URL",
            "key": "i"
        }, {
            "val": "View inline image",
            "key": "l"
        }, {
            "val": "Save inline image to file",
            "key": "[ESC] [l]"
        }, {
            "val": "Mark URL-like strings as anchors",
            "key": ":"
        }, {
            "val": "Mark Message-ID-like strings as news anchors",
            "key": "[ESC] [:]"
        }, {
            "val": "Peek current URL",
            "key": "c"
        }, {
            "val": "Display information about current document",
            "key": "="
        }, {
            "val": "Show current line number",
            "key": "C-g"
        }, {
            "val": "View history of URL",
            "key": "C-h"
        }, {
            "val": "Render frame",
            "key": "F"
        }, {
            "val": "Browse current document using external browser (use 2M and 3M to invoke second and third browser)",
            "key": "M"
        }, {
            "val": "Browse link using external browser (use 2ESC M and 3ESC M to invoke second and third browser)",
            "key": "[ESC] [M]"
        }],
        "File/Stream operation": [{
            "val": "Open URL",
            "key": "U"
        }, {
            "val": "View new file",
            "key": "V"
        }, {
            "val": "Execute shell command and load",
            "key": "@"
        }, {
            "val": "Execute shell command and browse",
            "key": "#"
        }],
        "Buffer operation": [{
            "val": "Back to the previous buffer",
            "key": "B"
        }, {
            "val": "View HTML source",
            "key": "v"
        }, {
            "val": "Select buffer",
            "key": "s"
        }, {
            "val": "Edit buffer source",
            "key": "E"
        }, {
            "val": "Redraw screen",
            "key": "C-l"
        }, {
            "val": "Reload buffer",
            "key": "R"
        }, {
            "val": "Save buffer",
            "key": "S"
        }, {
            "val": "Save source",
            "key": "[ESC] [s]"
        }, {
            "val": "Edit buffer image",
            "key": "[ESC] [e]"
        }],
        "Buffer selection mode": [{
            "val": "Select previous buffer",
            "key": "[k], [C-p]"
        }, {
            "val": "Select next buffer",
            "key": "[j], [C-n]"
        }, {
            "val": "Delect current buffer",
            "key": "D"
        }, {
            "val": "Go to the selected buffer",
            "key": "RET"
        }],
        "Bookmark operation": [{
            "val": "Load bookmark",
            "key": "[ESC] [b]"
        }, {
            "val": "Add current to bookmark",
            "key": "[ESC] [a]"
        }],
        "Search": [{
            "val": "Search forward",
            "key": "[\\\\], [C-s]"
        }, {
            "val": "Search backward",
            "key": "[?], [C-r]"
        }, {
            "val": "Search next",
            "key": "n"
        }, {
            "val": "Search previous",
            "key": "N"
        }, {
            "val": "Toggle wrap search mode",
            "key": "C-w"
        }],
        "Mark operation": [{
            "val": "Set/unset mark",
            "key": "C-SPC"
        }, {
            "val": "Go to previous mark",
            "key": "[ESC] [p]"
        }, {
            "val": "Go to next mark",
            "key": "[ESC] [n]"
        }, {
            "val": "Mark by regular expression",
            "key": "\""
        }],
        "Miscellany": [{
            "val": "Execute shell command",
            "key": "!"
        }, {
            "val": "Help (load this file)",
            "key": "H"
        }, {
            "val": "Set option",
            "key": "o"
        }, {
            "val": "Show cookie jar",
            "key": "C-k"
        }, {
            "val": "Stop",
            "key": "C-c"
        }, {
            "val": "Suspend",
            "key": "C-z"
        }, {
            "val": "Quit (with confirmation, if you like)",
            "key": "q"
        }, {
            "val": "Quit without confirmation",
            "key": "Q"
        }],
        "Line-edit mode": [{
            "val": "Move cursor forward",
            "key": "C-f"
        }, {
            "val": "Move cursor backward",
            "key": "C-b"
        }, {
            "val": "Delete previous character",
            "key": "C-h"
        }, {
            "val": "Delete current character",
            "key": "C-d"
        }, {
            "val": "Kill everything after cursor",
            "key": "C-k"
        }, {
            "val": "Kill everything before cursor",
            "key": "C-u"
        }, {
            "val": "Move to the top of line",
            "key": "C-a"
        }, {
            "val": "Move to the bottom of line",
            "key": "C-e"
        }, {
            "val": "Fetch the previous string from the history list",
            "key": "C-p"
        }, {
            "val": "Fetch the next string from the history list",
            "key": "C-n"
        }, {
            "val": "Complete filename",
            "key": "[TAB], [SPC]"
        }, {
            "val": "Accept",
            "key": "RET"
        }]
    }
}
