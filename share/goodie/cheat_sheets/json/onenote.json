      "General (Windows)":[  
         {  
            "val":"Open a new OneNote window.",
            "key":"[Ctrl] [M]"
         },
         {  
            "val":"Open a small OneNote window to create a side note.",
            "key":"[Ctrl] [Shift] [M] or [Windows] [Alt] [N]"
         },
         {  
            "val":"Undo the last action.",
            "key":"[Ctrl] [Z]"
         },
         {  
            "val":"Redo the last action.",
            "key":"[Ctrl] [Y]"
         },
         {  
            "val":"Select all items on the current page.",
            "key":"[Ctrl] [A]"
         },
         {  
            "val":"Cut the selected text or item.",
            "key":"[Ctrl] [X]"
         },
         {  
            "val":"Copy the selected text or item to the Clipboard.",
            "key":"[Ctrl] [C]"
         },
         {  
            "val":"Paste the contents of the Clipboard.",
            "key":"[Ctrl] [V]"
         },
         {  
            "val":"Check spelling.",
            "key":"F7"
