{
    "id": "latex_document_writing_cheat_sheet",
    "name": "LatexDocumentWritingCheatSheet",
    "description": "Basic Commands for Latex Document",

    "metadata": {
        "sourceName": "ShareLaTex",
        "sourceUrl" : "https://www.sharelatex.com/learn"
    },

    "aliases": [
        "latex", "latex document", "latex editor"
    ],

    "template_type": "code",

    "section_order": [
        "Starting A New Document",
        "Document Classes",
        "Title",
        "Formatting",
        "Lists",
        "Special Characters"
    ],

    "sections": {
        "Starting A New Document": [
            {
                "key": "\\documentation\\{class\\}",
                "val": "Declares type of document you are writing; Must be at the top of document"
            },
            {
                "key": "\\begin\\{document\\}",
                "val": "Starts a document"
            },
            {
                "key": "\\end\\{document\\}",
                "val": "Ends a document"
            }
        ],
        "Document Classes": [
            {
                "key": "article",
                "val": "For short documents and journal articles. Is the most commonly used"
            },
            {
                "key": "IEEEtran",
                "val": "Articles with the IEEE Transactions format"
            },
            {
                "key": "report",
                "val": "Longer reports with several chapters, short books, thesis"
            },
            {
                "key": "book",
                "val": "Full length books"
            },
            {
                "key": "slides",
                "val": "Slides using big sans serif font"
            },
            {
                "key": "letter",
                "val": "Letter writing"
            }
        ],
        "Title": [
            {
                "key": "\\title\\{Document's title\\}",
                "val": "Title of document; Must go before \\begin{document}"
            },
            {
                "key": "\\author\\{name\\}",
                "val": "Author of document; Must go before \\begin{document}"
            },
            {
                "key": "\\date\\{date\\}",
                "val": "Date of document, Optional; Must go before \\begin{document}"
            },
            {
                "key": "\\maketitle",
                "val": "Produces title; Goes after \\begin{document}"
            }
        ],
         "Formatting": [
            {
                "key": "\\\\\\",
                "val": "Newline"
            },
            {
                "key": "\\pagebreak",
                "val": "Begin new page"    
            },
            {
                "key": "\\textbf\\{your text\\}",
                "val": "Bold text"
            },
            {
                "key": "\\underline\\{your text\\}",
                "val": "Underlined text"
            },
            {
                "key": "\\textit\\{your text\\}",
                "val": "Italicized text"
            },
            {
                "key": "\\verb|<your text>|",
                "val": "Verbatim text"
            },
            {
                "key": "\\par",
                "val": "Starts a new paragraph"
            },
            {
                "key": "\\begin\\{flushleft\\}<your text>\\end\\{flushleft\\}",
                "val": "Left align text"
            },
            {
                "key": "\\begin\\{flushright\\}<your text>\\end\\{flushright\\}",
                "val": "Right align text"
            },
            {
                "key": "\\begin\\{center\\}<your text>\\end\\{center\\}",
                "val": "Center align text"
            } 
        ],
        "Lists": [
            {
                "key": "\\begin\\{itemize\\}",
                "val": "Begin unordered list"
            },
            {
                "key": "\\end\\{itemize\\}",
                "val": "End unordered list"
            },
            {
                "key": "\\begin\\{enumerate\\}",
                "val": "Begin ordered list"
            },
            {
                "key": "\\end\\{enumerate\\}",
                "val": "End ordered list"
            },
            {
                "key": "\\item",
                "val": "All list entry must be preceded by \\item"
            }
        ],
        "Special Characters": [
            {
                "key": "`<your text>'",
                "val": "Single Quote"
            },
            {
                "key": "``<your text>''",
                "val": "Double Quote"
            },
            {
                "key": "\\#",
                "val": "Pound sign: #"
            },
            {
                "key": "\\$",
                "val": "Dollar sign: $"
            },
            {
                "key": "\\%",
                "val": "Percent sign: %"
            },
            {
                "key": "\\&",
                "val": "Ampersand: &"
            }
        ]
    }
}
