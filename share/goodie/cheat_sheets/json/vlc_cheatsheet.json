    "View": [
      {
        "val": "Increase font size",
        "key": "[Ctrl] ( [+] / [=] )"
      },
      {
        "val": "Decrease font size",
        "key": "[Ctrl] [-]"
      },
      {
        "val": "Restore font size",
        "key": "[Ctrl] [0]"
      },
      {
        "val": "Hide sidebar",
        "key": "[Ctrl] [Shift] [H]"
      },
      {
        "val": "Scroll line down",
        "key": "[Ctrl] [↓]"
      },
      {
        "val": "Scroll line up",
        "key": "[Ctrl] [↑]"
      }
