      {
        "val": "Indent",
        "key": "[Ctrl] [\\[]"
      },
      {
        "val": "Move line(s) down",
        "key": "[Ctrl] [Shift] [↓]"
      },
      {
        "val": "Open line below",
        "key": "[Ctrl] [Enter]"
