      {
        "val": "Jump to definition",
        "key": "[Ctrl] [J]"
      },
      {
        "val": "Quick docs",
        "key": "[Ctrl] [K]"
      },
      {
        "val": "New rule",
        "key": "[Ctrl] [Alt] [N]"
      },
      {
        "val": "Quick open",
        "key": "[Ctrl] [Shift] [O]"
      },
      {
        "val": "Next match",
        "key": "[Alt] [↓]"
      },
      {
        "val": "Go to first error/warning",
        "key": "F8"
      },
      {
        "val": "Previous document",
        "key": "[Ctrl] [Shift] [Tab]"
      },
      {
        "val": "Next document",
        "key": "[Ctrl] [Tab]"
