      {
        "val": "Find",
        "key": "[Ctrl] [F]"
      },
      {
        "val": "Find in files",
        "key": "[Ctrl] [Shift] [F]"
      },
      {
        "val": "Replace",
        "key": "[Ctrl] [H]"
      },
      {
        "val": "Paste",
        "key": "[Ctrl] [V]"
