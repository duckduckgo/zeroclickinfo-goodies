    "Files": [
      {
        "val": "Live highlight",
        "key": "[Ctrl] [Shift] [C]"
      },
      {
        "val": "New file",
        "key": "[Ctrl] [N]"
      },
      {
        "val": "Open file",
        "key": "[Ctrl] [O]"
      },
      {
        "val": "Live preview file",
        "key": "[Ctrl] [Alt] [P]"
      },
      {
        "val": "Save file",
        "key": "[Ctrl] [S]"
