            {
                "key": "H(x) = F(x)*G(x)",
                "val": "H'(x) = F'(x)*G(x) + F(x)*G'(x)"
            }
        ],
        "Chain Rule": [
            {
                "key": "H(x) = F( G(x) )",
                "val": "H'(x) = F'( G(x) )*G'(x)"
