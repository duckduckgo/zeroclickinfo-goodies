        "Chat": [   
            {
                "key": "[Ctrl] [F4]",
                "val": "Show chat window"
            },  
            {
                "key": "Tab",
                "val": "Focus chat window"
            },                 
            {
                "key": "F8",
                "val": "Show whisper dialog"
            }      
        ],           
        "World Interaction": [
            {
                "key": "Left-Click",
                "val": "Activate object"
            },
            {
                "key": "Right-Click",
                "val": "Inspect object (opens object properties dialog)"
            },      
            {
                "key": "[Shift] [Right-Click]",
