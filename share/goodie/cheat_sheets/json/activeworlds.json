            {
                "key": "F12",
                "val": "Detach movers"
            },
            {
                "key": "Shift",
                "val": "Pass through solid objects"
            }              
        ],
        "Browser": [
            {
                "key": "[Ctrl] [F2]",
                "val": "Show toolbar"
            },
            {
                "key": "F3",
                "val": "Show downloads"
            },        
            {
                "key": "[Ctrl] [F3]",
                "val": "Show gestures"
            },
            {
                "key": "[Ctrl] [F5]",
                "val": "Show status bar"
            },
            {
                "key": "[Ctrl] [F7]",
