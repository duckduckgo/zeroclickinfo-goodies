            },
            {
                "key": "Numpad -",
                "val": "Move up (fly)"
            },     
            {
                "key": "Numpad +",
                "val": "Move down (fly)"
            },    
            {
                "key": "Numpad 5",
                "val": "Stop"
            }              
        ],
        "Camera": [
            {
                "key": "[Page Up] / [Numpad 9]",
                "val": "Look up"
            },
            {
                "key": "[Page Down] / [Numpad 3]",
                "val": "Look down"
            },
            {
                "key": "[Home] [Numpad 7]",
                "val": "First person view"
            },          
            {
                "key": "[End] / [Numpad 1]",
