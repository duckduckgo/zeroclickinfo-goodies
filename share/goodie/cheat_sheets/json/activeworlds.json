{
    "id": "activeworlds_cheat_sheet",
    "name": "Activeworlds",
    "description": "Keyboard shortcuts for ActiveWorlds, a virtual worlds software",

    "metadata": {
        "sourceName": "AWBuilder",
        "sourceUrl" : "http://awbuilder.blogspot.com/2016/01/reference-activeworlds-keyboard-defaults.html"
    },

    "aliases": [
        "active worlds", "alphaworld"
    ],

    "template_type": "keyboard",

    "section_order": [
        "Avatar Movement",
        "World Interaction",
        "Object Properties",   
        "Camera",
        "Browser",        
        "Chat"
    ],

    "sections": {
        "Avatar Movement": [
            {
                "key": "[↑] / [numpad 8]",
                "val": "Move avatar forward"
            },
            {
                "key": "[↓] / [numpad 2]",
                "val": "Move avatar backward"
            },
            {
                "key": "[←] / [numpad 4]",
                "val": "Turn avatar left"
            },
            {
                "key": "[→] / [numpad 6]",
                "val": "Turn avatar right"
            },
            {
                "key": "[left shift] / [right shift]",
                "val": "Sidestep"
            },
            {
                "key": "ctrl",
                "val": "Run"
            },
            {
                "key": "[left alt] [f12] / [numpad 0]",
                "val": "Jump"
            },
            {
                "key": "[numpad -]",
                "val": "Move up (fly)"
            },     
            {
                "key": "[numpad +]",
                "val": "Move down (fly)"
            },    
            {
                "key": "[numpad 5]",
                "val": "Stop"
            }              
        ],
        "Camera": [
            {
                "key": "[page up] / [numpad 9]",
                "val": "Look up"
            },
            {
                "key": "[page down] / [numpad 3]",
                "val": "Look down"
            },
            {
                "key": "[home] [numpad 7]",
                "val": "First person view"
            },          
            {
                "key": "[end] / [numpad 1]",
                "val": "Change camera view"
            },       
            {
                "key": "[numpad *]",
                "val": "Camera zoom in"
            },        
            {
                "key": "[numpad /]",
                "val": "Camera zoom out"
            },      
            {
                "key": "[middle mouse]",
                "val": "Toggle mouselook mode on / off"
            }       
        ],
        "Chat": [   
            {
                "key": "[ctrl] [f4]",
                "val": "Show chat window"
            },  
            {
                "key": "tab",
                "val": "Focus chat window"
            },                 
            {
                "key": "f8",
                "val": "Show whisper dialog"
            }      
        ],           
        "World Interaction": [
            {
                "key": "[left mouse]",
                "val": "Activate object"
            },
            {
                "key": "[right mouse]",
                "val": "Inspect object (opens object properties dialog)"
            },      
            {
                "key": "[shift] [right mouse]",
                "val": "Inspect multiple objects (opens object properties dialog)"
            },      
            {
                "key": "[ctrl] [shift] [right mouse]",
                "val": "Inspect all objects in a cell (opens object properties dialog)"
            },    
            {
                "key": "f10",
                "val": "Show cell grid"
            },        
            {
                "key": "f11",
                "val": "Show your position"
            },              
            {
                "key": "f12",
                "val": "Detach movers"
            },
            {
                "key": "shift",
                "val": "Pass through solid objects"
            }              
        ],
        "Browser": [
            {
                "key": "[ctrl] [f2]",
                "val": "Show toolbar"
            },
            {
                "key": "f3",
                "val": "Show downloads"
            },        
            {
                "key": "[ctrl] [f3]",
                "val": "Show gestures"
            },
            {
                "key": "[ctrl] [f5]",
                "val": "Show status bar"
            },
            {
                "key": "[ctrl] [f7]",
                "val": "Show credit info"
            },
            {
                "key": "f7",
                "val": "Show web browser"
            },   
            {
                "key": "f9",
                "val": "Show tabs"
            }         
        ],     
        "Object Properties": [
            {
                "key": "delete",
                "val": "Delete current selection"
            },
            {
                "key": "[numpad 0]",
                "val": "Duplicate current selection"
            },     
            {
                "key": "↑",
                "val": "Move current selection forward"
            },
            {
                "key": "↓",
                "val": "Move current selection back"
            },
            {
                "key": "←",
                "val": "Move current selection left"
            },
            {
                "key": "→",
                "val": "Move current selection right"
            },            
            {
                "key": "[numpad +]",
                "val": "Move current selection up"
            },
            {
                "key": "[numpad -]",
                "val": "Move current selection down"
            },      
            {
                "key": "[numpad /]",
                "val": "Tilt current selection 15 degrees on +x axis"
            },     
            {
                "key": "[numpad *]",
                "val": "Tilt current selection 15 degrees on -x axis"
            },  
            {
                "key": "[page down]",
                "val": "Rotate current selection 15 degrees on +y axis"
            },  
            {
                "key": "[page up]",
                "val": "Rotate current selection 15 degrees on -y axis"
            },  
            {
                "key": "home",
                "val": "Roll current selection 15 degrees on +z axis"
            },  
            {
                "key": "end",
                "val": "Roll current selection 15 degrees on -z axis"
            }             
        ]         
    }
}
