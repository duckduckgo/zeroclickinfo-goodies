		"Tag property": [{
			"key": "'none'",
			"val": "Unspecified tag (automatically resolved by application)."
		}, {
			"key": "'!'",
			"val": "Non-specific tag (by default, \"!!map\"/\"!!seq\"/\"!!str\")."
		}, {
			"key": "'!foo'",
			"val": "Primary (by convention, means a local \"!foo\" tag)."
		}, {
			"key": "'!!foo'",
			"val": "Secondary (by convention, means \"tag:yaml.org,2002:foo\")."
		}, {
			"key": "'!h!foo'",
			"val": "Requires \"%TAG !h! <prefix>\" (and then means \"<prefix>foo\")."
		}, {
			"key": "'!<foo>'",
			"val": "Verbatim tag (always means \"foo\")."
		}],
